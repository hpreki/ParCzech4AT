  <pcz:meta pcz:name="udpipe2.prefix-pdt">
    <pcz:item pcz:xpath="/tei:TEI/tei:teiHeader/tei:encodingDesc/tei:listPrefixDef">
      <pcz:test>
        <pcz:false pcz:xpath="/tei:TEI/tei:teiHeader/tei:encodingDesc/tei:listPrefixDef/tei:prefixDef[@tei:ident='pdt']" />
      </pcz:test>
      <pcz:tei>
        <prefixDef ident="pdt" matchPattern="(.+)" replacementPattern="../pdt-fslib.xml#xpath(//fvLib/fs[./f/symbol/@value = '$1'])">
          <p>Feature-structure elements definition of the Czech Positional Tags</p>
        </prefixDef>
      </pcz:tei>
    </pcz:item>
  </pcz:meta>

  <pcz:meta pcz:name="udpipe2.prefix-ud-syn">
    <pcz:item pcz:xpath="/tei:TEI/tei:teiHeader/tei:encodingDesc/tei:listPrefixDef">
